      <fo:block id="sec:menu_machine"><fo:block><fo:block><fo:block margin-left="0pt" font-family="sans-serif,Symbol,ZapfDingbats" keep-together.within-column="always"><fo:block keep-with-next.within-column="always"><fo:block font-family="sans-serif,Symbol,ZapfDingbats" font-weight="bold" keep-with-next.within-column="always" space-before.minimum="0.8em" space-before.optimum="1.0em" space-before.maximum="1.2em" text-align="start" start-indent="0pt"><fo:marker marker-class-name="section.head.marker">Machine</fo:marker><fo:block font-size="17.28pt">2.1.4.Â Machine<fo:wrapper id="idp1385015652"><!--Machine--></fo:wrapper></fo:block></fo:block></fo:block></fo:block></fo:block></fo:block>
        
